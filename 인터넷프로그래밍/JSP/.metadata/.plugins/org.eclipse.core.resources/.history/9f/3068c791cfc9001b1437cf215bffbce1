<!-- myBatis를 통해 DB를 다룰때 사용할 파일이다. 실행하고자 할 쿼리 모음 -->

<mapper namespace="sqlMap.mapper">
	<select id="selectList" resultMap="src.user.User">
		select *
		from user
	</select>
	<select id=""></select>

</mapper>